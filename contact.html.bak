	 <div class="outerWrap">
    <div class="innerWrap">
        
		
    </div>
	<form id="ContactMe" action="http://jacquelynricophtotography@example.com" method="post">
	  <fieldset id="custInfo">
	  
		<legend>Book your session</legend>
		
	<div class="formRow">
		<label for="name">Name*</label>
		<input name="custName" id="name" type="text" placeholder="first and last name" required />
	</div>
				
		<div class="formRow">
			<label for="city">City </label>
			<input name="custCity" id="city"  type="text" value="Omaha area"  />
		</div>
		
		<div class="formRow">			
			<label for="state">State </label>
			<input name="custState" id="state"  type="text" value="NE or IA" />
		</div>
			
		<div class="formRow">
			<label for="phone">Phone Number</label>
			<input name="custphone" id="phone" type="tel" placeholder="(nnn) nnn-nnnn"
			pattern="^\d{10}$ ^(\(\d{3}\)\s*)?\d{3}[\s-]?\d{4}$" />
		</div>
				
		<div class="formRow">	
			<label for="mail">Email*</label>
			<input name="custEmail" id="mail" type="email" required/>
		</div>
			
		<div class="formRow">
			<label for="info">Where did you hear Jacquelyn Rico Photography?<br />
			(select all that apply)</label>
			<select name="infoSrc" id="info" size="5" multiple>
				<option value="internet">Internet</option>
				<option value= "mag">Instagram</option>
				<option value="news">Facebook</option>
				<option value="word">Friends</option>
				<option value="other">Other</option> 
			</select>
		</div>
		<aside>
	<figure>
	<img src="Images/mommy-small.jpg" alt="mommy and me"/>
	 </figure>
	 </aside>
			
		<div class="formRow">
				<label for="contactme">What memories are you looking to capture?</label>
				<input name="Memory" id="Memories" type="text" list="MemoriesCaptured"/>
				<datalist id="Memories2">
					<option value="Newborn">
					<option value="Senior">
					<option value="Families">
					<option value="Simply I Do">
					<option value="Purely I DO ">
					<option value="Truly I DO">
				</datalist>
		</div>
				
		<div class="formRow">
				<label for="serviceDate">Expected date of session? </label>
				<input name="SessionDate" id="Date" type="Date"
					 step="1" max="10" />
		</div>
			<label for="commaBox">Tell me more about your perfect session</label>
			<textarea name="custExp" id="commBox"></textarea>
			
		</fieldset>
		
		<div id="buttons">
			<input type="submit" value="Book My Session" />
			<input type="reset" value="Cancel" />
		</div>
	</form>	

	
